{
    "manifest_version": 2,
    "name": "Auto Join Zoom",
    "version": "1.0",
    "description": "It can join your zoom classes for you!",
    "permissions": ["activeTab","storage", "<all_urls>","tabs","alarms","background"],

    "background": {
        "scripts":["background.js"],
        "persistent":false
    },

    "browser_action":{
        "default_popup":"popup.html"
    }
        

}